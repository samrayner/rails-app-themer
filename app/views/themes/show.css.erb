/* Theme template generated by running `rake theme:update`
   More info: https://github.com/samrayner/rails-app-themer */

.themed {
  background-color: <%= theme.color(:background) %>;
  background-image: <%= theme.image_url(:background) %>;
  color: <%= theme.color(:text) %>;
  font-family: <%= theme.font(:body) %>;
}
.themed h1 {
  font-family: <%= theme.font(:headings) %>;
}
.image {
  background: <%= theme.image_url(:logo) %> no-repeat 0 0;
}
